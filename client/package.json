{
  "name": "react-template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config=webpack.config.js",
    "dev-no-update": "cross-env NODE_ENV=development webpack serve --config=webpack.config.js",
    "clean:prod": "rm -rf build && rm -rf dist && mkdir dist",
    "clean:prod:win": "rmdir /s /q build && rmdir /s /q dist && mkdir dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build": "npm run clean:prod && npm run lint && npm run build:webpack",
    "build:win": "npm run clean:prod:win && npm run lint && npm run build:webpack",
    "reinstall-module": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "lint": "eslint src",
    "publish:development": "cd .. && npm run publish:development",
    "publish:staging": "cd .. && npm run publish:staging",
    "publish:production": "cd .. && npm run publish:production",
    "pub:dev": "cd .. && npm run pub:dev",
    "pub:staging": "cd .. && npm run pub:staging",
    "pub:prod": "cd .. && npm run pub:prod",
    "pub:all": "cd .. && npm run pub:all"
  },
  "devDependencies": {
    "@loadable/component": "^5.16.4",
    "@pdg/api": "^1.0.19",
    "@pdg/react-hook": "^1.0.26",
    "@pdg/util": "^1.0.23",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@types/jest": "^29.5.14",
    "@types/loadable__component": "^5.13.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "js-file-download": "^0.4.12",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0",
    "react-router-dom": "^7.5.0",
    "react-use-error-boundary": "^3.0.0",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.17",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.3",
    "url-loader": "^4.1.1",
    "usehooks-ts": "^3.1.1",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
